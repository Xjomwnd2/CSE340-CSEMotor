<!DOCTYPE html>
<html lang="en">
    <%- include('../partials/head') %>
    <body>
        <div id="wrapper">
            <%- include('../partials/header') %>
            <%- include('../partials/navigation') %>
            <!-- Navigation -->
    <nav>
        <ul>
            <% if (vehicles && vehicles.length) { %>
                <% vehicles.forEach(function(vehicle) { %>
                    <li><a href="<%= vehicle.link %>"><%= vehicle.name %></a></li>
                <% }); %>
            <% } %>
        </ul>
    </nav>
            <main>
                <%- body %>
            </main>
            <%- include('../partials/footer') %>
        </div>
    </body>
    </html>

